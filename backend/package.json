{
  "name": "fleetflow-backend",
  "version": "0.0.1",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext ts",
    "db:generate": "prisma generate --schema=../database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=../database/prisma/schema.prisma",
    "db:seed": "tsx src/scripts/seed_indian_data.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.15.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.10.1",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.9",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "json2csv": "^6.0.0-alpha.2",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
