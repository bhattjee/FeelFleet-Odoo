# PostgreSQL (use the same DB you use in pgAdmin)
# Example: postgresql://USER:PASSWORD@localhost:5432/DATABASE_NAME
DATABASE_URL="postgresql://postgres:yourpassword@localhost:5432/fleetflow"

# JWT (use a long random string in production)
JWT_SECRET="your-super-secret-key-at-least-16-chars"
JWT_EXPIRES_IN="24h"

# Server
PORT=4000

# Frontend origin(s) for CORS - comma-separated if multiple (e.g. different Vite port)
CORS_ORIGIN="http://localhost:5173,http://localhost:5174"

NODE_ENV=development
